{
  "env" :{
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "globals": {
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "curly": 2,
    "no-var": 2,
    "semi": [1, "always"],
    "strict": [2, "global"]
  }
}