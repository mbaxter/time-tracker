{
  "name": "time-tracker",
  "version": "0.0.0",
  "description": "A productivity app for tracking and managing time.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node build/backend/server",
    "heroku-postbuild": "npm --production=false install && npm run build-server && npm run fixtures && npm run build-frontend",
    "dev-server": "node build/backend/server",
    "dev-server-watch": "nodemon build/backend/server --watch build",
    "prod-server-start": "$(npm bin)/forever start build/backend/server",
    "prod-server-stop": "$(npm bin)/forever stop build/backend/server",
    "build-server": "npm run build-server-verbose > /dev/null",
    "build-server-verbose": "$(npm bin)/babel src --out-dir build",
    "build-server-watch": "$(npm bin)/babel src --watch --out-dir build",
    "build-frontend": "webpack",
    "build-frontend-watch": "webpack --progress --colors --watch",
    "fixtures": "node build/backend/scripts/load-default-db-fixtures.js",
    "lint": "$(npm bin)/eslint {src,test}/*",
    "test": "$(npm bin)/mocha -R spec --compilers js:babel-core/register --require babel-polyfill --recursive test/suites"
  },
  "author": "M. Baxter",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-bluebird": "0.1.1",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-react": "6.6.0",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "postcss-loader": "1.1.1",
    "promise-mysql": "2.0.0",
    "redux-logger": "2.7.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.3"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "bcrypt": "0.8.7",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "cls-bluebird": "2.0.1",
    "continuation-local-storage": "3.2.0",
    "email-validator": "1.0.7",
    "express": "4.14.0",
    "express-handlebars": "3.0.0",
    "forever": "0.15.2",
    "http-status-codes": "1.0.6",
    "humanize-string": "1.0.1",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.1.1",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.16.4",
    "moment": "2.15.2",
    "moment-timezone": "0.5.7",
    "mysql": "2.11.1",
    "node-env-file": "0.1.8",
    "node-uuid": "1.4.7",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "sequelize": "3.24.4"
  }
}
