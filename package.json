{
  "name": "time-tracker",
  "version": "0.0.0",
  "description": "A productivity app for tracking and managing time.",
  "main": "src/server/index.js",
  "scripts": {
    "dev-server": "node src/server",
    "dev-server-watch": "nodemon src/server --watch src/server",
    "prod-server-start": "$(npm bin)/forever start src/server",
    "prod-server-stop": "$(npm bin)/forever stop src/server",
    "lint": "$(npm bin)/eslint {src,test}/*",
    "test": "$(npm bin)/mocha -R spec --compilers js:babel-core/register --recursive test"
  },
  "author": "M. Baxter",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "browserify": "13.1.1",
    "eslint": "3.8.1",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "promise-mysql": "2.0.0"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "forever": "0.15.2",
    "http-status-codes": "1.0.6",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.16.4",
    "mysql": "2.11.1",
    "node-env-file": "0.1.8",
    "sequelize": "3.24.4"
  }
}
