{
  "name": "time-tracker",
  "version": "0.0.0",
  "description": "A productivity app for tracking and managing time.",
  "main": "src/server/index.js",
  "scripts": {
    "dev-server": "node build/backend/server",
    "dev-server-watch": "nodemon build/backend/server --watch build",
    "prod-server-start": "$(npm bin)/forever start build/backend/server",
    "prod-server-stop": "$(npm bin)/forever stop build/backend/server",
    "build-server": "npm run build-server-verbose > /dev/null",
    "build-server-verbose": "babel src --out-dir build --ignore src/frontend",
    "build-server-watch": "babel src --watch --out-dir build --ignore src/frontend",
    "build-frontend-dev": "npm run build-frontend -- -t [envify --API_URL http://localhost:3000/api --NODE_ENV dev]",
    "build-frontend-watch-dev": "npm run build-frontend-watch -- -t [envify --API_URL http://localhost:3000/api --NODE_ENV dev]",
    "build-frontend": "browserify ./src/frontend/index.js -o ./public/js/app.js",
    "build-frontend-watch": "watchify ./src/frontend/index.js -o ./public/js/app.js",
    "fixtures": "node build/backend/scripts/load-default-db-fixtures.js",
    "lint": "$(npm bin)/eslint {src,test}/*",
    "test": "$(npm bin)/mocha -R spec --compilers js:babel-core/register --require babel-polyfill --recursive test/suites"
  },
  "author": "M. Baxter",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.18.0",
    "babel-plugin-transform-bluebird": "0.1.1",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "browserify-shim": "3.8.12",
    "envify": "3.4.1",
    "eslint": "3.8.1",
    "eslint-plugin-react": "6.6.0",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "promise-mysql": "2.0.0",
    "reactify": "1.1.1",
    "redux-logger": "2.7.4",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "bcrypt": "0.8.7",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "cls-bluebird": "2.0.1",
    "continuation-local-storage": "3.2.0",
    "email-validator": "1.0.7",
    "express": "4.14.0",
    "forever": "0.15.2",
    "http-status-codes": "1.0.6",
    "humanize-string": "1.0.1",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.16.4",
    "moment": "2.15.2",
    "moment-timezone": "0.5.7",
    "mysql": "2.11.1",
    "node-env-file": "0.1.8",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "sequelize": "3.24.4"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$",
    "moment": "global:moment",
    "moment-timezone": "global:moment",
    "react": "global:React",
    "react-dom": "global:ReactDOM",
    "react-router": "global:ReactRouter"
  }
}
