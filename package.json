{
  "name": "time-tracker",
  "version": "0.0.0",
  "description": "A productivity app for tracking and managing time.",
  "main": "src/server/index.js",
  "scripts": {
    "dev-server": "node build/backend/server",
    "dev-server-watch": "nodemon build/backend/server --watch build",
    "prod-server-start": "$(npm bin)/forever start build/backend/server",
    "prod-server-stop": "$(npm bin)/forever stop build/backend/server",
    "build-server": "npm run build-server-verbose > /dev/null",
    "build-server-verbose": "babel src --out-dir build --ignore src/frontend",
    "build-server-watch": "babel src --watch --out-dir build --ignore src/frontend",
    "fixtures": "npm run build-server && node build/backend/scripts/load-default-db-fixtures.js",
    "lint": "$(npm bin)/eslint {src,test}/*",
    "test": "$(npm bin)/mocha -R spec --compilers js:babel-core/register --require babel-polyfill --recursive test/suites"
  },
  "author": "M. Baxter",
  "license": "ISC",
  "devDependencies": {
    "eslint": "3.8.1",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "promise-mysql": "2.0.0"
  },
  "dependencies": {
    "babel-core": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-polyfill": "6.16.0",
    "bcrypt": "0.8.7",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "browserify": "13.1.1",
    "email-validator": "1.0.7",
    "express": "4.14.0",
    "forever": "0.15.2",
    "http-status-codes": "1.0.6",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.16.4",
    "moment": "2.15.2",
    "moment-timezone": "0.5.7",
    "mysql": "2.11.1",
    "node-env-file": "0.1.8",
    "sequelize": "3.24.4"
  }
}
