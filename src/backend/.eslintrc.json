{
  "env" :{
    "commonjs": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "curly": 2,
    "no-console": 0,
    "no-var": 2,
    "strict": [2, "global"]
  }
}